"*":
  Ensime:
    sbtExec: "/Users/ches/bin/sbt"
    typecheckWhen: "save"
  "atom-ctags":
    cmd: "ctags"
    cmdArgs: "--tag-relative --recurse -f .git/tags --exclude=.git"
    extraTagFiles: ".git/tags"
  "atom-typescript": {}
  "autocomplete-erlang":
    elixirPath: "/usr/local/bin/elixir"
  "autocomplete-php": {}
  "autocomplete-plus": {}
  "autocomplete-python":
    useKite: false
    useSnippets: "required"
  "autocomplete-ruby":
    rsensePath: "~/.gem/ruby/2.3.1/bin/rsense"
  "base16-syntax":
    scheme: "Tomorrow"
  "better-git-blame":
    colorScale: "ChocolateMint"
    searchInLayerEnabled: false
    sendUsageStatistics: false
  build:
    stealFocus: false
  "build-cargo": {}
  core:
    disabledPackages: [
      "linter-gotype"
      "sourcegraph-atom"
      "platformio-ide-terminal"
      "platformio-ide"
      "linter-scalac"
      "symbols-view"
      "tabs"
      "atom-typescript"
      "linter-tslint"
      "build-tsc"
      "autocomplete-ruby"
      "ginkgo-and-gomega-snippets"
      "Ensime"
      "git-blame"
      "vim-mode"
      "vim-surround"
    ]
    packagesWithKeymapsDisabled: [
      "racer"
    ]
    projectHome: "~/src"
    telemetryConsent: "limited"
    themes: [
      "nucleus-dark-ui"
      "gruvbox"
    ]
  dash:
    grammars:
      Python: [
        "py"
      ]
  docblockr:
    indentation_spaces_same_para: 3
  editor:
    fontFamily: "Inconsolata"
    invisibles: {}
    lineHeight: 1.4
    scrollPastEnd: true
    softWrapAtPreferredLineLength: true
    tabLength: 4
  "ex-mode":
    gdefault: true
    splitbelow: true
    splitright: true
  "exception-reporting":
    userId: "9f35f77d-b182-15f2-cad2-98458372d0e0"
  "git-blame":
    colorCommitAuthors: true
    columnWidth: 205
    showLastNames: false
  "git-diff":
    showIconsInEditorGutter: true
  "git-plus": {}
  "go-config":
    gopath: "~/src/go"
  "go-oracle":
    oraclePath: "~/src/go/bin/oracle"
  "go-plus":
    config: {}
    panelDisplayMode: "closed"
  "haskell-ghc-mod":
    additionalPathDirectories: [
      "~/.local/bin"
      "~/.cabal/bin"
    ]
  "highlight-line":
    enableBackgroundColor: false
    underline: "dotted"
  "ide-haskell":
    cabalPath: "~/.cabal/bin/cabal"
    onCursorMove: "Show Tooltip"
  "isotope-ui":
    backgroundGradient: true
    fontFamily: "Fira Sans"
  linter: {}
  "linter-gotype": {}
  "linter-pep8": {}
  "linter-phpcs": {}
  "linter-ui-default":
    panelHeight: 96
    showPanel: true
  "markdown-preview": {}
  "merge-conflicts":
    gitPath: "/usr/local/bin/git"
  "one-dark-ui": {}
  "platformio-ide":
    useBuiltinPlatformIO: false
  "platformio-ide-terminal":
    core: {}
  "project-manager":
    sortBy: "group"
  racer:
    racerBinPath: "~/.cargo/bin/racer"
    rustSrcPath: "~/src/rust/rust/src/"
  "seti-ui":
    compactView: true
  "sourcegraph-atom":
    highlightReferencesInFile: false
  "split-diff":
    colors: {}
    hideDocks: true
  "status-bar": {}
  "symbols-tree-view": {}
  tabs:
    alwaysShowTabBar: false
    enableVcsColoring: true
  "terminal-plus":
    core: {}
    style: {}
  "tool-bar":
    position: "Right"
  "tree-view":
    hideVcsIgnoredFiles: true
  "vim-mode":
    useSmartcaseForSearch: true
  "vim-mode-plus":
    defaultScrollRowsOnMiniScroll: 2
    hideCommandsFromCommandPalette: true
    sequentialPaste: true
    sequentialPasteMaxHistory: 6
    statusBarModeStringStyle: "long"
    useSmartcaseForSearch: true
  "vim-surround":
    surroundCommand: "S"
  welcome:
    showOnStartup: false
".erlang.html.text.yaws":
  editor:
    tabLength: 4
".erlang.source":
  editor:
    tabLength: 4
".lua.source.tts":
  editor:
    tabLength: 4
".scala.source":
  editor:
    tabLength: 2
".source.ts":
  editor:
    tabLength: 4
".source.tsx":
  editor:
    tabLength: 4
